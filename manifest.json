{
  "manifest_version": 2,
  "name": "crossOriginIsolatedDemo",
  "description": "Demo for cross origin isolated",
  "version": "0.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}

